{
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "dev": "nuxt dev -p 3333",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "prepare": "is-ci || husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "nuxi typecheck",
    "run:eslint": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint/ --ext .ts,.vue,.json,.yaml,.yml .",
    "run:stylelint": "stylelint --fix --cache --cache-location ./node_modules/.cache/stylelint/ '**/*.{css,scss,vue}'",
    "lint": "pnpm run:eslint && pnpm run:stylelint",
    "format": "prettier -uw --cache ."
  },
  "dependencies": {
    "@headlessui/vue": "1.7.10",
    "@pinia/nuxt": "0.4.6"
  },
  "devDependencies": {
    "@histoire/plugin-nuxt": "0.15.4",
    "@histoire/plugin-vue": "0.15.3",
    "@nuxt/test-utils-edge": "3.0.0-rc.9-27703329.537338f",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/tailwindcss": "6.4.0",
    "@testing-library/vue": "6.6.1",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitest/coverage-c8": "0.28.5",
    "@vueuse/core": "9.12.0",
    "@vueuse/nuxt": "9.12.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsonc": "2.6.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-yml": "1.5.0",
    "happy-dom": "8.4.2",
    "histoire": "0.14.2",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.2",
    "msw": "1.0.1",
    "nuxt": "3.2.0",
    "nuxt-icon": "0.2.11",
    "postcss-html": "1.5.0",
    "prettier": "2.8.4",
    "sass": "1.58.1",
    "stylelint": "15.1.0",
    "stylelint-config-css-modules": "4.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-standard-scss": "7.0.1",
    "stylelint-config-standard-vue": "1.0.0",
    "typescript": "4.9.5",
    "vitest": "0.28.5",
    "vue-tsc": "1.1.0"
  },
  "peerDependencies": {
    "@histoire/plugin-vue": "0.15.3",
    "histoire": "0.14.2"
  },
  "lint-staged": {
    "*.{ts,vue,json,yaml,yml}": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint/",
    "*.{css,scss,vue}": "stylelint --fix --cache --cache-location ./node_modules/.cache/stylelint/",
    "*": "prettier -wu --cache",
    "*.{test,spec}.ts": "vitest related"
  }
}
